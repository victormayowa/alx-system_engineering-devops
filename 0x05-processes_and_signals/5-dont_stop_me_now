#!/usr/bin/env bash
# This script sends a SIGTERM signal to stop the process started by 4-to_infinity_and_beyond.

# Find and kill the process named "4-to_infinity_and_beyond"
pid=$(pgrep -x "4-to_infinity_and_beyond")
if [ -n "$pid" ]; then
    kill "$pid"
    echo "Process terminated"
else
    echo "Process not found"
fi
